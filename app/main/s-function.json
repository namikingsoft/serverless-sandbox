{
  "name": "main",
  "runtime": "nodejs",
  "description": "Serverless Lambda function for project: serverless-sandbox",
  "customName": false,
  "customRole": false,
  "handler": "main/handler.handler",
  "timeout": 300,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "main",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponses}"
    },
    {
      "path": "main/{id}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponses}"
    },
    {
      "path": "main",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponses}"
    },
    {
      "path": "main/{id}",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponses}"
    }
  ],
  "events": [
    {
      "name": "crontask",
      "type": "schedule",
      "config": {
        "schedule": "rate(5 minutes)"
      }
    }
  ],
  "environment": {
    "SERVERLESS_PROJECT": "serverless-sandbox",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}

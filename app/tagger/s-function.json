{
  "name": "tagger",
  "runtime": "nodejs",
  "description": "Serverless Lambda function for project: serverless-sandbox",
  "customName": false,
  "customRole": false,
  "handler": "tagger/handler.handler",
  "timeout": 300,
  "memorySize": 1028,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "tagger/{text}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponses}"
    },
    {
      "path": "tagger",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponses}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "serverless-sandbox",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}

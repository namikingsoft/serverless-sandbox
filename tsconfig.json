{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "moduleResolution": "node",
    "baseUrl": "./app/src",
    "outDir": "./app/lib",
    "strictNullChecks": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "test",
    "typings",
    "node_modules",
    "app/node_modules",
    "_meta"
  ]
}
